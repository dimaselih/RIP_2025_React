{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "cors-fetch:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://192.168.1.211/**" },
        { "url": "https://192.168.1.211/**" },
        { "url": "http://localhost/**" },
        { "url": "https://localhost/**" }
      ],
      "deny": []
    },
    "core:default",
    "http:allow-fetch",
    "http:allow-fetch-read-body",
    "http:allow-fetch-send"
  ]
}
