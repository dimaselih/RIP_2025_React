{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "cors-fetch:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://localhost/**" },
        { "url": "http://127.0.0.1/**" },
        { "url": "http://192.168.*/**" },
        { "url": "http://10.*/**" },
        { "url": "http://172.*/**" },
        { "url": "https://localhost/**" },
        { "url": "https://127.0.0.1/**" },
        { "url": "https://192.168.*/**" },
        { "url": "https://10.*/**" },
        { "url": "https://172.*/**" }
      ],
      "deny": []
    },
    "core:default",
    "http:allow-fetch",
    "http:allow-fetch-read-body",
    "http:allow-fetch-send"
  ]
}
